{
  "name": "denystroshuk/sorted-linked-list",
  "description": "Sorted singly linked list (int or string) - small library",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "DenysTroshuk\\SortedLinkedList\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DenysTroshuk\\SortedLinkedList\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^4",
    "phpstan/phpstan": "^1.10"
  },
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs",
    "fix": "php-cs-fixer fix",
    "stan": "./vendor/bin/phpstan analyse src tests"
  },
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
